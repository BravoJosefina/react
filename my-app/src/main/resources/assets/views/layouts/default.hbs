{{!-- footer --}}
{{> partials/uitk/footer hf}}

{{!-- Scripts --}}
{{> partials/uitk/core-js}}

{{!-- Include the 'app' bundle generated by FrontendBuilder --}}
{{{includeJsBundle bundleName="app" assetPath=EG.appHostedAssetPath}}}

{{!-- See "more-scripts" in views/my-app/sample.hbs to learn more --}}
{{{block "more-scripts"}}}
</body>
</html>
